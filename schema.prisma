datasource postgresql {
  url      = env("DATABASE_URL")
  provider = "postgresql"
}

generator client {
  provider = "prisma-client-js"
  output   = "node_modules/.prisma/client"
}

model Resource {
  id          String   @id @default(cuid())
  key         String?  @unique
  value       String?
  isMultiline Boolean?
  maxLength   Int?
  pages       Page?    @relation("Resource_pages", fields: [pagesId], references: [id])
  pagesId     String?  @map("pages")

  @@index([pagesId])
}

model Page {
  id          String     @id @default(cuid())
  name        String?
  url         String?    @unique
  description String?
  resources   Resource[] @relation("Resource_pages")
}